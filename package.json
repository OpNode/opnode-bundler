{
  "name": "transeptor-bundle",
  "version": "0.1.0",
  "description": "A modular Typescript ERC-4337 Bundler, designed with a strong emphasis on performance",
  "main": "execute.ts",
  "scripts": {
    "///ab": "_____________________________________________________________________",
    "///cv": "________________________BUNDLER MODES________________________________",
    "start:base": "ts-node src/execute.ts --httpApi web3,eth,debug --txMode base | pino-pretty",
    "start:base:unsafe": "ts-node src/execute.ts --httpApi web3,eth,debug --txMode base  --unsafe | pino-pretty",
    "start:conditional": "ts-node src/execute.ts --httpApi web3,eth,debug --txMode conditional | pino-pretty",
    "start:conditional:unsafe": "ts-node src/execute.ts --httpApi web3,eth,debug --txMode conditional  --unsafe | pino-pretty",
    "start:searcher": "ts-node src/execute.ts --network https://eth-goerli.g.alchemy.com/v2 --txMode searcher --httpApi web3,eth,debug --unsafe | pino-pretty",
    "///id": "_____________________________________________________________________",
    "///oa": "________________________DEV SCRIPTS__________________________________",
    "build": "npx webpack",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "geth:start": "docker-compose up geth",
    "geth:stop": "docker-compose stop geth",
    "deploy:local": "hardhat deploy --network localhost",
    "transeptor:start": "docker-compose up transeptor",
    "transeptor:stop": "docker-compose stop transeptor",
    "signer:balance": "hardhat run ./scripts/check-signer-balance.ts --network localhost",
    "fetch:abis": "ts-node scripts/abis.ts",
    "print": "hardhat run ./scripts/print-deterministic-addr.ts --network localhost",
    "fund": "hardhat run ./scripts/fund-account.ts --network localhost",
    "arch-diagram": "npx arkit -f src/execute.ts -o transeptor-architecture.svg ",
    "///tu": "_____________________________________________________________________",
    "///iu": "________________________TEST/LINT SCRIPTS_____________________________",
    "test": "hardhat test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": {
    "name": "Transeptor Labs",
    "email": "bowmanidris95@gmail.com",
    "url": "https://transeptorlabs.io"
  },
  "license": "GNU",
  "dependencies": {
    "@account-abstraction/sdk": "^0.6.0",
    "async-mutex": "0.4.0",
    "commander": "10.0.1",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "ethers": "^5.7.1",
    "express": "4.18.2",
    "helmet": "6.1.5",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "^0.6.0",
    "@nomicfoundation/hardhat-network-helpers": "1.0.0",
    "@nomicfoundation/hardhat-toolbox": "3.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "3.0.0",
    "@typechain/ethers-v5": "10.1.0",
    "@typechain/hardhat": "6.1.2",
    "@types/chai": "^4.3.5",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/helmet": "4.0.0",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "arkit": "1.6.4",
    "chai": "^4.3.7",
    "eslint": "8.39.0",
    "hardhat": "2.11.0",
    "hardhat-deploy": "^0.11.11",
    "jest": "^29.5.0",
    "pino-pretty": "^10.0.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "webpack": "5.82.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "5.0.2",
    "webpack-node-externals": "^3.0.0"
  }
}
