{
  "name": "root",
  "private": true,
  "version": "0.6.2-alpha.0",
  "description": "A ERC4337 bundler node written in typescript",
  "author": {
    "name": "Transeptor Labs",
    "email": "transeptorhq@gmail.com",
    "url": "https://transeptorlabs.io"
  },
  "license": "GNU",
  "homepage": "https://github.com/transeptorlabs/transeptor-bundler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transeptorlabs/transeptor-bundler.git"
  },
  "bugs": {
    "url": "https://github.com/transeptorlabs/transeptor-bundler/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "///ab": "_____________________________________________________________________",
    "///cv": "________________________WELCOME BUILDERS______________________________",
    "///tu": "_____________________________________________________________________",
    "clean": "rm -rf dist",
    "build": "yarn workspaces foreach --parallel --interlaced --verbose run build",
    "dev": "yarn workspaces foreach --parallel --interlaced --verbose run dev",
    "start": "yarn workspaces foreach --parallel --interlaced --verbose run start",
    "start:prod": "yarn workspaces foreach --parallel --interlaced --verbose run start:prod",
    "test": "yarn workspaces foreach --parallel --interlaced --verbose run test",
    "lint": "yarn workspaces foreach --parallel --interlaced --verbose run lint",
    "lint:fix": "yarn workspaces foreach --parallel --interlaced --verbose run lint:fix",
    "op": "node --loader ts-node/esm ./e2e/send-userop.e2e.ts | pino-pretty",
    "build:contracts": "cd contracts && forge build",
    "///cK": "________________________DOCKER CONTAINERS______________________________",
    "local-eth": "./eth-node",
    "image:bundle-builder": "docker build -t bundle-builder:v-local --build-arg PACKAGE_NAME=bundle-builder .",
    "image:relayer": "docker build -f Dockerfile.relayer -t relayer:v-local .",
    "start:docker-bundle-builder": "docker-compose up bundle-builder",
    "start:docker-relayer": "docker-compose up relayer",
    "influxdb": "docker-compose up influxdb",
    "grafana": "docker-compose up grafana"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "18.16.3",
    "@types/os-utils": "^0.0.4",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.39.0",
    "nodemon": "^3.1.0",
    "pino-pretty": "^10.0.0",
    "ts-node": "10.9.1",
    "tsc": "^2.0.4",
    "typescript": "5.0.4",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=20.11.1"
  }
}
