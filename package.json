  },
  "license": "GNU",
  "homepage": "https://github.com/transeptorlabs/transeptor-bundler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transeptorlabs/transeptor-bundler.git"
  },
  "bugs": {
    "url": "https://github.com/transeptorlabs/transeptor-bundler/issues"
  },
  "private": true,
  "scripts": {
    "eth-node": "cd packages/tools && npm run geth:start",
    "bundler-prep": "cd packages/tools && npm run bundler-startup",
    "bundler:base": "cd packages/bundler && npm run start:base",
    "bundler:base-unsafe": "cd packages/bundler && npm run start:base:unsafe",
    "bundler:base-unsafe-auto": "cd packages/bundler && npm run start:base:unsafe-auto",
    "bundler:conditional": "cd packages/bundler && npm run start:conditional",
    "bundler:conditional-unsafe": "cd packages/bundler && npm run start:conditional:unsafe",
    "bundler:searcher": "cd packages/bundler && npm run start:searcher",
    "bundler:base-p2p": "cd packages/bundler && npm run start:base-p2p",
    "bundler:peer": "cd packages/bundler && npm run start:base-peer",
    "test": "cd packages/bundler && npm run test",
    "lint": "cd packages/bundler && npm run lint",
    "lint-bundler-fix": "cd packages/bundler && npm run lint:fix",
    "build": "cd packages/bundler && npm run build",
    "start:prod": "cd packages/bundler && npm run start:prod",
    "build:bundler-docker": "docker build -t bundler-typescript:v-local .",
    "start:bundler-docker": "cd packages/tools && npm run start:docker-bundler"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
