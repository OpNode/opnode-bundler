{
  "name": "root",
  "private": true,
  "version": "0.6.2-alpha.0",
  "description": "A fast ERC-4337 bundler node with TypeScript-based compile-time type safety.",
  "author": {
    "name": "Transeptor Labs",
    "email": "transeptorhq@gmail.com",
    "url": "https://transeptorlabs.io"
  },
  "license": "GNU",
  "homepage": "https://github.com/transeptorlabs/transeptor-bundler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transeptorlabs/transeptor-bundler.git"
  },
  "bugs": {
    "url": "https://github.com/transeptorlabs/transeptor-bundler/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn workspaces foreach --parallel --interlaced --verbose run build",
    "dev": "yarn workspaces foreach --parallel --interlaced --verbose run dev",
    "start": "yarn workspaces foreach --parallel --interlaced --verbose run start",
    "start:prod": "yarn workspaces foreach --parallel --interlaced --verbose run start:prod",
    "test": "yarn workspaces foreach --parallel --interlaced --verbose run test",
    "lint": "yarn workspaces foreach --parallel --interlaced --verbose run lint",
    "lint:fix": "yarn workspaces foreach --parallel --interlaced --verbose run lint:fix",
    "build:contracts": "cd contracts && forge build",
    "///gK": "________________________RELAYER NODE______________________________",
    "send-op": "yarn workspace bundler-relayer run send-op",
    "relayer-dev": "yarn workspace bundler-relayer run dev",
    "relayer-lint": "yarn workspace bundler-relayer run lint",
    "relayer-lint:fix": "yarn workspace bundler-relayer run lint:fix",
    "relayer-test": "yarn workspace bundler-relayer run test",
    "image:relayer": "docker build -f Dockerfile.bundler-relayer -t bundler-relayer:v-local .",
    "///gj": "________________________Builder NODE______________________________",
    "builder-dev": "yarn workspace bundler-builder run dev",
    "builder-lint": "yarn workspace bundler-builder run lint",
    "builder-lint:fix": "yarn workspace bundler-builder run lint:fix",
    "builder-test": "yarn workspace bundler-builder run test",
    "image:builder": "docker build -f Dockerfile.bundler-builder -t bundler-builder:v-local .",
    "test:mempool-race": "yarn workspace bundler-builder run test:mempool-race",
    "///sj": "________________________Shared MODULE ______________________________",
    "shared-test": "yarn workspace shared run test",
    "shared-lint": "yarn workspace shared run lint",
    "shared-lint:fix": "yarn workspace shared run lint:fix",
    "///cK": "________________________DOCKER CONTAINERS______________________________",
    "start:images": "docker-compose up -d bundler-relayer",
    "stop:images": "docker-compose down",
    "build:images": "yarn image:relayer && yarn image:builder",
    "local-eth": "./eth-node",
    "influxdb": "docker-compose up influxdb",
    "grafana": "docker-compose up grafana"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "18.16.3",
    "@types/os-utils": "^0.0.4",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=20.11.1"
  }
}
