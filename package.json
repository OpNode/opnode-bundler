    "bundler:peer": "cd packages/bundler && npm run start:base-peer",
    "bundler:base-unsafe": "cd packages/bundler && npm run start:base:unsafe",
    "bundler:conditional": "cd packages/bundler && npm run start:conditional",
    "bundler:conditional-safe": "cd packages/bundler && npm run start:conditional:unsafe",
    "bundler:searcher": "cd packages/bundler && npm run start:searcher",
    "test-bundler": "cd packages/bundler && npm run test",
    "lint-bundler": "cd packages/bundler && npm run lint",
    "build-bundler-esm": "cd packages/bundler && npm run build",
    "start:prod": "cd packages/bundler && npm run start:prod-esm"
