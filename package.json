{
  "name": "transeptor-bundler",
  "version": "0.5.3-alpha.0",
  "description": "A lightweight ERC4337 bundler node designed specifically for Node.js environment; with a focus on minimal resource consumption, high performance, and minimal dependencies.",
  "author": {
    "name": "Transeptor Labs",
    "email": "transeptorhq@gmail.com",
    "url": "https://transeptorlabs.io"
  },
  "license": "GNU",
  "homepage": "https://github.com/transeptorlabs/transeptor-bundler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transeptorlabs/transeptor-bundler.git"
  },
  "bugs": {
    "url": "https://github.com/transeptorlabs/transeptor-bundler/issues"
  },
  "scripts": {
    "///ab": "_____________________________________________________________________",
    "///cv": "________________________WELCOME BUILDERS______________________________",
    "///tu": "_____________________________________________________________________",
    "clean": "rm -rf dist",
    "build": "tsc -p ./tsconfig.json",
    "build:contracts": "cd contracts && forge build",
    "start:dev": "npm run build && node dist/cjs/src/index.js --httpApi web3,eth,debug --txMode base | pino-pretty",
    "start:dev-metrics": "npm run build && node dist/cjs/src/index.js --httpApi web3,eth,debug --txMode base --metrics | pino-pretty",
    "test": "jest --coverage --config ./jest.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "local-eth": "./dev-node",
    "///cK": "________________________DOCKER CONTAINERS______________________________",
    "build:image": "docker build -t bundler-typescript:v-local .",
    "influxdb": "docker-compose up influxdb",
    "grafana": "docker-compose up grafana",
    "start:docker-bundler": "docker-compose up bundler-typescript-local"
  },
  "dependencies": {
    "@account-abstraction/sdk": "^0.6.0",
    "@chainsafe/libp2p-noise": "^12.0.2",
    "@influxdata/influxdb-client": "1.33.2",
    "@libp2p/mplex": "^8.0.4",
    "@libp2p/tcp": "^7.0.3",
    "@multiformats/multiaddr": "^12.1.5",
    "async-mutex": "0.4.0",
    "commander": "10.0.1",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "ethers": "^5.7.1",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "libp2p": "^0.45.9",
    "os-utils": "0.0.14",
    "pino": "^8.14.1"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "^0.6.0",
    "@types/jest": "^29.5.2",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.39.0",
    "jest": "^29.5.0",
    "pino-pretty": "^10.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
