{
  "name": "@transeptor/bundler",
  "version": "0.2.0",
  "description": "A modular Typescript ERC-4337 Bundler, designed with a strong emphasis on performance",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "module": "dist/bundler.js",
  "homepage": "https://github.com/transeptorlabs/transeptor-bundler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transeptorlabs/transeptor-bundler.git"
  },
  "bugs": {
    "url": "https://github.com/transeptorlabs/transeptor-bundler/issues"
  },
  "private": true,
  "scripts": {
    "///ab": "_____________________________________________________________________",
    "///cv": "________________________BUNDLER DEV __________________________________",
    "start:base": "ts-node --experimental-specifier-resolution=node src/execute.ts --httpApi web3,eth,debug --txMode base | pino-pretty",
    "start:base:unsafe": "ts-node --experimental-specifier-resolution=node src/execute.ts --httpApi web3,eth,debug --txMode base --unsafe | pino-pretty",
    "start:base:unsafe-auto": "ts-node --experimental-specifier-resolution=node src/execute.ts --httpApi web3,eth,debug --txMode base --unsafe --auto | pino-pretty",
    "start:conditional": "ts-node --experimental-specifier-resolution=node src/execute.ts --httpApi web3,eth,debug --txMode conditional | pino-pretty",
    "start:conditional:unsafe": "ts-node --experimental-specifier-resolution=node src/execute.ts --httpApi web3,eth,debug --txMode conditional  --unsafe | pino-pretty",
    "start:searcher": "ts-node --experimental-specifier-resolution=node src/execute.ts --network https://eth-goerli.g.alchemy.com/v2 --txMode searcher --httpApi web3,eth,debug --unsafe | pino-pretty",
    "start:base-p2p": "ts-node --experimental-specifier-resolution=node src/execute.ts --httpApi web3,eth,debug --txMode base  --unsafe --p2p | pino-pretty",
    "start:base-peer": "ts-node --experimental-specifier-resolution=node src/execute.ts --httpApi web3,eth,debug --txMode base  --unsafe --p2p --findPeers --port 3001| pino-pretty",
    "///ar": "_____________________________________________________________________",
    "///ch": "________________________BUNDLER PROD __________________________________",
    "build": "tsc",
    "start:prod": "node --experimental-specifier-resolution=node dist/src/execute.js --httpApi web3,eth,debug --txMode base",
    "transeptor:start": "docker-compose up transeptor",
    "transeptor:stop": "docker-compose stop transeptor",
    "///tu": "_____________________________________________________________________",
    "///iu": "________________________TEST/LINT SCRIPTS_____________________________",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "arch-diagram": "npx arkit -f src/execute.ts -o transeptor-architecture.svg "
  },
  "author": {
    "name": "Transeptor Labs",
    "email": "transeptorhq@gmail.com",
    "url": "https://transeptorlabs.io"
  },
  "license": "GNU",
  "dependencies": {
    "@account-abstraction/sdk": "^0.6.0",
    "@chainsafe/libp2p-noise": "^12.0.2",
    "@libp2p/mplex": "^8.0.4",
    "@libp2p/tcp": "^7.0.3",
    "@multiformats/multiaddr": "^12.1.5",
    "async-mutex": "0.4.0",
    "commander": "10.0.1",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "ethers": "^5.7.1",
    "express": "4.18.2",
    "helmet": "6.1.5",
    "libp2p": "^0.45.9",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "^0.6.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/helmet": "4.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "arkit": "1.6.4",
    "eslint": "8.39.0",
    "jest": "^29.5.0",
    "pino-pretty": "^10.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "../bundler",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/modules/types/**/*.ts",
      "!src/modules/config/**/*.ts",
      "!src/modules/cli/**/*.ts",
      "!src/modules/validation/GethTracer.ts",
      "!src/modules/validation/parseScannerResult.ts",
      "!src/modules/validation/BundlerCollectorTracer.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
