    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/transeptorlabs/transeptor-bundler.git"
    },
    "bugs": {
        "url": "https://github.com/transeptorlabs/transeptor-bundler/issues"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
      "lib/**/*.d.ts",
      "lib/**/*.js",
