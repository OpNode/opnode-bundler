        "utils": "*",
        "bundle": "*",
        "event": "*",
        "json-rpc-handler": "*",
        "json-rpc-server": "*",
        "mempool": "*",
        "provider": "*",
        "reputation": "*",
        "validation": "*"
    },
    "devDependencies": {
    }
